{
  "type": "extension",
  "name": "bytedance_tts_duplex",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "=0.6.22"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "interface": [{
      "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
    }],
    "property": {
      "properties": {
        "appid": {
          "type": "string"
        },
        "token": {
          "type": "string"
        },
        "voice_type": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int64"
        },
        "api_url": {
          "type": "string"
        }
      }
    }
  }
}