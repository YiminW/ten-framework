{
  "type": "extension",
  "name": "azure_asr_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "=0.6.22"
    }
  ],
  "api": {
    "interface": [{
      "import_uri": "../../system/ten_ai_base/api/asr-interface.json"
    }],
    "property": {
      "properties": {
        "api_key": {
          "type": "string"
        },
        "region": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "language": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int64"
        }
      }
    }
  }
}